	//flex 布局
	"graceUI padding":{
		"prefix" : "gpadding",
		"body"   : ["<view class=\"grace-padding\">\n\t$1\n</view>"]
	},
	"grace Flex" :{
		"prefix" : "gFlex",
		"body"   : ["<view class=\"grace-flex\">\n\t$1\n</view>"]
	},
	"grace Flex Wrap" :{
		"prefix" : "gFlexWrap",
		"body"   : ["<view class=\"grace-wrap\">\n\t$1\n</view>"]
	},
	"grace Flex NoWrap" :{
		"prefix" : "gFlexNoWrap",
		"body"   : ["<view class=\"grace-nowrap\">\n\t$1\n</view>"]
	},
	"grace Flex Space Between" :{
		"prefix" : "gFlexSpaceBetween",
