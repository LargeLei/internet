	"graceUI slider [区间滑块]" : {
		"prefix" :"gslider",
		"body"   : ["<graceSlider min=\"1\" max=\"100\" :val1=\"valMin\" :val2=\"valMax\" v-on:sliding=\"sliding1\"></graceSlider>",
			"<!-- 请在 js 脚本区域加入  -->",
			"import graceSlider from '../../graceUI/components/graceSlider.vue';",
			"<!-- 在 js data 中加入 -->",
			"valMin : 20,",
			"valMax : 55",
			"<!-- 添加函数  -->",
			"methods:{",
			"	sliding1 : function(e){",
			"		var min = e.min;",
			"		var max = e.max;",
			"		this.valMin = min;",
			"		this.valMax = max;",
			"	}",
			"}",
			"<!-- 在 js 中注册组件 -->",
			"components : {",
			"	graceSlider",
			"}"
		]
	},
