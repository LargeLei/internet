	},
	"graceUI scrollX [横向滚动]":{
		"prefix" :"gscrllx",
		"body"   : ["<scroll-view class=\"grace-scroll-x\" scroll-x>",
			"\t<view class=\"grace-items\" style=\"background:#F00;\">\n\t\t$1\n\t</view>",
			"\t<view class=\"grace-items\" style=\"background:#0F0;\">\n\t\t$1\n\t</view>",
			"\t<view class=\"grace-items\" style=\"background:#00F;\">\n\t\t$1\n\t</view>",
