			"	</navigator>",
			"	<!-- 多图模式 -->",
			"	<navigator url=\"\">",
			"		<view class=\"grace-news-list-img-news\">",
			"	    	<view class=\"grace-news-list-title-main\">标题</view>",
			"			<view class=\"grace-news-list-imgs\">",
			"				<image src=\"../../static/$1\" mode=\"widthFix\"></image>",
			"				<image src=\"../../static/$1\" mode=\"widthFix\"></image>",
			"				<image src=\"../../static/$1\" mode=\"widthFix\"></image>",
			"	    	</view>",
			"		</view>",
			"		<view class=\"grace-news-list-info\">",
			"			<view>",
			"				<text class=\"grace-iconfont icon-time2\"></text>2天前",
			"				<text class=\"grace-iconfont icon-eye\" style=\"margin-left:20px;\"></text>99次浏览",
			"			</view>",
			"		</view>",
			"	</navigator>",
			"</view>"]
	},
	"graceUI tab [局部选项卡]" : {
		"prefix" : "gtab",
		"body"   : ["<view class=\"grace-tab\">",
			"	<scroll-view class=\"grace-tab-title\" scroll-x=\"true\">",
			"		<view v-for=\"(tab, index) in tabs\" :class=\"[tabCurrentIndex == index ? 'grace-tab-current' : '']\"  :id=\"'tabTag-'+index\" @tap=\"tabChange\" :key=\"index\">{{tab.name}}</view>",
			"	</scroll-view>",
			"	<swiper class=\"grace-tab-swiper\" :current=\"swiperCurrentIndex\" @change=\"swiperChange\" style=\"height:350rpx;\">",
			"		<swiper-item>内容区 - 1</swiper-item>",
			"		<swiper-item>内容区 - 2</swiper-item>",	
			"		<swiper-item>内容区 - 3</swiper-item>",
			"		<swiper-item>内容区 - 4</swiper-item>",
			"		<swiper-item>内容区 - 5</swiper-item>",
			"		<swiper-item>内容区 - 6</swiper-item>",
			"	</swiper>",
			"</view>",
			"<!-- 请在 script 标签顶层 输入代码块 gtabdata 并将代码移动到 data :{} -->",
			"<!-- 请在 script 标签顶层 输入代码块 gtabmethods 并将代码移动到 methods :{} -->"]
	},
