			"				<view class=\"grace-news-list-title-main\">标题</view>",
			"				<text class=\"grace-news-list-title-desc grace-text-overflow\">描述</text>",
			"			</view>",
			"			<view class=\"grace-news-tips grace-news-tips-l\">标签</view>",
			"		</view>",
			"	</navigator>",
			"	<!-- 文左图右模式 -->",
			"	<navigator url=\"\">",
			"		<view class=\"grace-news-list-items\">",
			"			<view class=\"grace-news-list-title\">",
			"				<view class=\"grace-news-list-title-main\">标题</view>",
			"				<text class=\"grace-news-list-title-desc grace-text-overflow\">描述</text>",
			"			</view>",
			"			<image src=\"../../static/$1\" class=\"grace-news-list-img grace-list-imgs-r\" mode=\"widthFix\"></image>",
			"			<view class=\"grace-news-tips grace-news-tips-r\">新闻</view>",
			"		</view>",
			"	</navigator>",
			"	<!-- 大图模式 -->",
			"	<navigator url=\"\">",
			"		<view class=\"grace-news-list-img-news\">",
			"			<view class=\"grace-news-list-title-main\">标题</view>",
			"			<view class=\"grace-news-list-img-big\">",
			"				<image src=\"../../static/$1\" mode=\"widthFix\"></image>",
			"			</view>",
			"		</view>",
